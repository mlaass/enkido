{
  "name": "enkido-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bun scripts/build-docs-index.ts && bunx --bun vite build",
    "build:docs": "bun scripts/build-docs-index.ts",
    "build:wasm": "cd wasm && sh ./build.sh",
    "preview": "bunx --bun vite preview",
    "check": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.0",
    "@codemirror/commands": "^6.7.0",
    "@codemirror/lang-javascript": "^6.2.0",
    "@codemirror/language": "^6.10.0",
    "@codemirror/lint": "^6.9.2",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.34.0",
    "codemirror": "^6.0.0",
    "gray-matter": "^4.0.3",
    "lucide-svelte": "^0.562.0",
    "marked": "^17.0.1"
  }
}
